{
    "workbench.colorTheme": "Default Light Modern",
    "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-light",
    "workbench.fontAliasing": "auto",
    "workbench.activityBar.location": "top",
    "workbench.activityBar.orientation": "vertical",
    "workbench.activityBar.iconSize": 16,
    "vscode_custom_css.imports": [
        "file://${userHome}/Library/Application Support/Cursor/User/settings.css"
    ],
    "editor.fontSize": 13,
    "editor.fontFamily": "JetBrains Mono SemiBold",
    "editor.fontLigatures": true,
    "editor.lineHeight": 2,
    "editor.fontVariations": false,
    "debug.console.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "JetBrains Mono SemiBold",
    "chat.editor.fontSize": 13,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.formatOnType": true,
    "editor.folding": true,
    "editor.tabCompletion": "on",
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.semanticTokenColorCustomizations": {},
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 300,
    "files.exclude": {
        "**/node_modules": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/.idea": true,
        "**/coverage": true
    },
    "window.titleBarStyle": "custom",
    "window.openFoldersInNewWindow": "on",
    "window.customTitleBarVisibility": "auto",
    "window.commandCenter": true,
    "window.restoreFullscreen": false,
    "window.restoreWindows": "folders",
    "window.newWindowDimensions": "inherit",
    "window.zoomLevel": 0,
    "window.density.editorTabHeight": "default",
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.limit.value": 15,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.enablePreview": false,
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "onHover",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsserver.maxTsServerMemory": 8192,
    "typescript.suggest.paths": false,
    "typescript.suggest.autoImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggest.paths": false,
    "javascript.suggest.autoImports": true,
    "vs-code-prettier-eslint.prettierLast": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "git.suggestSmartCommit": false,
    "git.confirmSync": false,
    "githubPullRequests.pullBranch": "never",
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.tabs.location": "left",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "sql.format.quotation": false,
    "sql.format.identifierCase": "none",
    "sql.format.keywordCase": "none",
    "cursor.aipreview.enabled": true,
    "colorIdentifiersMode.paletteMode": "automatic",
    "redhat.telemetry.enabled": false,
    "gitlens.telemetry.enabled": false,
    "explorer.confirmDelete": false,
    "mochaExplorer.env": {
        "ELECTRON_RUN_AS_NODE": null
    },
    "debug.showInStatusBar": "always"
}